{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> EditScaleDisplayElement: aBlElement scaleValue: aValue [

	self currentScaleFactor: aValue.
	pyramidSelectionWidgetExtension scaleFactor: self currentScaleFactor.
	pyramidPositionExtension scaleFactor: self currentScaleFactor.

	self refreshScaledZoom.

	self installOnPixelGrid.

	aBlElement transformDo: [ :t |
		t scaleBy: self currentScaleFactor.
		t translateBy: self scaledZoom ].
	
	slider blocToMoveZoomPosition: self currentScaleFactor
]

{ #category : #connecting }
PyramidSpaceZoomPlugin >> connectOn: aPyramidEditor [

	self pluginFromPyramid: aPyramidEditor.

	pyramidSelectionWidgetExtension ifNil: [ ^ self ].

	pyramidSelectionWidgetExtension scaleFactor: self currentScaleFactor.

	self installOnZoomSpace.

	sizeElementExtent := (pyramidSelectionWidgetExtension elementAtMain
		                      childWithId: #MainExtension_sizeElement)
		                     extent
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> createPixelGrid: aPoint [

	| nbWidthPixel aWidth realNbWidthPixel nbHeightPixel aHeight realNbHeightPixel pixelGrid sizeElement |
	
	pixelGridEnable ifTrue: [ ^ self ].
	
	pixelGridEnable := true.
	
	sizeElement := (pyramidSelectionWidgetExtension elementAtMain childWithId: #MainExtension_sizeElement).

	aWidth := aPoint x.
	aHeight := aPoint y.
	
	nbWidthPixel := aWidth / 200.
	realNbWidthPixel := nbWidthPixel rounded < nbWidthPixel
		                    ifTrue: [ nbWidthPixel rounded + 1 ]
		                    ifFalse: [ nbWidthPixel rounded ].
		
	nbHeightPixel := aHeight / 200.
	realNbHeightPixel := nbWidthPixel rounded < nbWidthPixel
		                    ifTrue: [ nbWidthPixel rounded + 1 ]
		                    ifFalse: [ nbWidthPixel rounded ].
	
	pixelGrid := BlElement new id: #pixelGrid.
	pixelGrid constraints horizontal matchParent.
	pixelGrid constraints vertical matchParent.

	0 to: realNbWidthPixel - 1 do: [ :x |
		0 to: realNbHeightPixel - 1 do: [ :y |
			| pixelElement |
			pixelElement := imagePixel asElement.
			pixelElement extent: 2010 @ 2010.
			pixelElement transformDo: [ :t | t scaleBy: 0.1 ].
			pixelElement position: x * 200 @ (y * 200) - 904.5.
			pixelElement clipChildren: false.
			pixelGrid addChild: pixelElement ] ].
	
	sizeElement addChild: pixelGrid.
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> createZoomSlider [

	slider := PyramidZoomSlider new.
	slider whenValueChangedDo: [ :scaleValue | 
		self EditScaleDisplayElement: pyramidSelectionWidgetExtension elementAtMain scaleValue: scaleValue ].
	
	slider scaledZoomValue: self maxScaleValue.
	slider sliderLapScale: self scaleLap.
	
	^ slider createSlider.
	
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> currentScaleFactor [

	^ currentScaleFactor
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> currentScaleFactor: aValue [

	currentScaleFactor := aValue.
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> defaultScale [

	self currentScaleFactor: 1.
	pyramidSelectionWidgetExtension ifNotNil: [
		self EditScaleDisplayElement: pyramidSelectionWidgetExtension elementAtMain scaleValue: 1 ]
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> defaultScaleButton [ 
	
	| button image |
	
	image := (BlElement new
         constraintsDo: [:constraints |  constraints horizontal exact: 16.0.
            constraints vertical exact: 16.0.
            constraints frame horizontal alignCenter.
            constraints frame vertical alignCenter ];
         background: (BlImageBackground new
               image: (PyramidExternalRessourceProxy new
                     pyramidExternalRessourceSource: (PyramidExternalRessourceSource new
                           target: Object;
                           selector: #iconNamed:;
                           arguments: {#smallFullscreen};
                           yourself);
                     yourself);
               opacity: 1.0;
               yourself)).

	button := BlElement new
   constraintsDo: [:constraints |  constraints horizontal exact: 20.0.
      constraints vertical exact: 20.0 ];
   background: (BlPaintBackground new
         paint: (BlColorPaint new
               color: (Color r: 1.0 g: 1.0 b: 1.0 alpha: 1.0);
               yourself);
         opacity: 1.0;
         yourself);
	border: Color black;
   layout: BlFrameLayout new;
   id: #defaultScaleButton;
	addEventHandler: (BlEventHandler
			 on: BlPrimaryMouseDownEvent
			 do: [ :evt | 
				self defaultScale.
				evt consumed: true ]).

	button addChild: image.
	
	^ button
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> display [

	^ display
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> display: aDisplay [

	display := aDisplay
]

{ #category : #initialization }
PyramidSpaceZoomPlugin >> initialize [

	self scaleLap: 0.1.
	self currentScaleFactor: 1.
	self maxScaleValue: 6.0.
	
	imagePixel := self class imagePixelGrid2000x2000BlackDot4x4.
	pixelGridEnable := false.
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> installOnPixelGrid [

	| sizeElement |
	sizeElement := pyramidSelectionWidgetExtension elementAtMain
		               childWithId: #MainExtension_sizeElement.
	
	sizeElementExtent := sizeElement extent.
	
	(self currentScaleFactor roundTo: 0.1) >= 5.5
		ifTrue: [ self createPixelGrid: sizeElementExtent ]
		ifFalse: [
			pixelGridEnable ifTrue: [
				pixelGridEnable := false.
				(pyramidSelectionWidgetExtension elementAtMain childWithId:
					 #MainExtension_sizeElement) removeChildWithId: #pixelGrid ] ]
]

{ #category : #displaying }
PyramidSpaceZoomPlugin >> installOnZoomSpace [

	pyramidSelectionWidgetExtension elementAtEvents addEventHandler:
		(BlEventHandler
			 on: BlMouseWheelEvent
			 do: [ :evt |
				 | moveTo |
				 moveTo := nil.
				 pyramidSelectionWidgetExtension isOnlyCtrlKeyPressed ifTrue: [
					 moveTo := evt vector asPoint leftRotated ].
				 moveTo ifNotNil: [
					 moveTo = (5 @ 0) asPoint
						 ifTrue: [ self zoomIn ]
						 ifFalse: [ self zoomOut ] ] ]).
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> maxScaleValue [

	^ maxScaleValue 
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> maxScaleValue: aScaleValue [

	maxScaleValue := aScaleValue 
]

{ #category : #'accessing - classes' }
PyramidSpaceZoomPlugin >> pluginFromPyramid: aPyramidEditor [

	| listOfSpaceExtension pyramidSelectionWidgetExtensionSet pyramidPositionExtensionSet listOfPlugins pluginSorted pyramidMainExtensionSet |
	
	listOfPlugins := aPyramidEditor plugins.
	pluginSorted := listOfPlugins select: [ :extensions | extensions isKindOf: PyramidSpacePlugin ].
	pluginSorted size = 1
	ifFalse: [^ self].
	spacePlugin := pluginSorted asArray first.
		
	listOfSpaceExtension := spacePlugin builder extensions.

	pyramidSelectionWidgetExtensionSet := listOfSpaceExtension select: [
		                                      :extensions |
		                                      extensions isKindOf:
			                                      PyramidSelectionWidgetExtension ].
	pyramidSelectionWidgetExtensionSet size = 1 ifFalse: [ ^ self ].
	pyramidSelectionWidgetExtension := pyramidSelectionWidgetExtensionSet
		                                   asArray first.

	pyramidPositionExtensionSet := listOfSpaceExtension select: [
		                               :extensions |
		                               extensions isKindOf:
			                               PyramidPositionExtension ].
	pyramidPositionExtensionSet size = 1 ifFalse: [ ^ self ].
	pyramidPositionExtension := pyramidPositionExtensionSet asArray first.
	
	pyramidMainExtensionSet := listOfSpaceExtension select: [
		                               :extensions |
		                               extensions isKindOf:
			                               PyramidMainExtension ].
	pyramidMainExtensionSet size = 1 ifFalse: [ ^ self ].

	pyramidMainExtension := pyramidMainExtensionSet asArray first
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> refreshPixelGrid: aPoint [

	| sizeElement |
	sizeElement := pyramidSelectionWidgetExtension elementAtMain
		               childWithId: #MainExtension_sizeElement.
	
	pixelGridEnable ifFalse: [ ^ self ].

	(sizeElementExtent = aPoint) ifTrue: [ ^ self ].
	sizeElementExtent := aPoint.

	pixelGridEnable := false.
	sizeElement removeChildWithId: #pixelGrid.
	
	(self currentScaleFactor roundTo: 0.1) >= 5.5
		ifTrue: [ self createPixelGrid: aPoint ]
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> refreshScaledZoom [

	scaledZoom := pyramidSelectionWidgetExtension elementAtMain extent x
	              * (self currentScaleFactor - 1 / 2)
	              @
		              (pyramidSelectionWidgetExtension elementAtMain extent y
		               * (self currentScaleFactor - 1 / 2))
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> scaleLap [

	^ scaleLap
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> scaleLap: aValue [
	
	self defaultScale.
	scaleLap := aValue
]

{ #category : #accessing }
PyramidSpaceZoomPlugin >> scaledZoom [

	^ scaledZoom
]

{ #category : #connecting }
PyramidSpaceZoomPlugin >> windowOpenBy: aPyramidEditor [

	| position aUIElement |
	
	aUIElement := BlElement new.
	aUIElement
		id: 'UI';
		height: 1;
		zIndex: 1001;
		clipChildren: false.
	aUIElement layout: BlLinearLayout new.
	aUIElement layout horizontalAlignment:
		BlElementAlignment horizontal start.
		
	pyramidSelectionWidgetExtension ifNil: [ ^ self ].

	position := pyramidSelectionWidgetExtension elementAtDisplaysAddons
		            childWithId: 'position'.

	pyramidSelectionWidgetExtension elementAtDisplaysAddons removeChild:
		position.
		
	pyramidSelectionWidgetExtension elementAtDisplaysAddons constraints
		horizontal matchParent.
	pyramidSelectionWidgetExtension elementAtDisplaysAddons
		height: 1;
		zIndex: 1001.

	aUIElement addChild: self createZoomSlider.
	aUIElement addChild: self defaultScaleButton.
	aUIElement addChild: position.

	pyramidSelectionWidgetExtension elementAtDisplaysAddons addChild:
		aUIElement.
		
	pyramidMainExtension workplacePropertiesView
		whenWorkplaceValuesChangedDo: [ :point |
			pyramidMainExtension extent: point.
			self refreshPixelGrid: point ]
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> zoomIn [

	self currentScaleFactor >= (slider sliderMaxScale - 0.05) ifTrue: [
		^ self ].

	self
		EditScaleDisplayElement:
		pyramidSelectionWidgetExtension elementAtMain
		scaleValue: self currentScaleFactor + scaleLap
]

{ #category : #'as yet unclassified' }
PyramidSpaceZoomPlugin >> zoomOut [

	self currentScaleFactor <= (scaleLap + slider sliderMinScale)
		ifTrue: [ ^ self ].

	self
		EditScaleDisplayElement:
		pyramidSelectionWidgetExtension elementAtMain
		scaleValue: self currentScaleFactor - scaleLap
]
