PyramidSpaceZoomPlugin class >> imagePixelGrid2000x2000BlackDot1x1 [ 
	<script: 'self imagePixelGrid2000x2000 inspect'>
	| base64 |
	base64 := ''.
	^ Form fromBinaryStream: base64 base64Decoded readStream.
]

